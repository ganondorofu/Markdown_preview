{
  "name": "popup-pro",
  "displayName": "Popup Pro",
  "description": "A VSCode extension that adds an activity bar button to trigger a popup message.",
  "version": "0.0.1",
  "publisher": "your-publisher-name",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/popup-pro.git"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:popup-pro.showPopup"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "popup-pro.showPopup",
        "title": "Show Popup Pro Message",
        "icon": "$(comment-discussion)"
      }
    ],
    "menus": {
      "activitybar": [
        {
          "command": "popup-pro.showPopup",
          "group": "navigation@1",
          "when": "true"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.6",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0"
  }
}
